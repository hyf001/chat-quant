{
    "name":"stock_zh_a_hist",
    "display_name":"历史行情数据",
    "description":"股日频率数据; 历史数据按日频率更新",
    "datasource":"akshare",
    "dir":"/A股",
    "input":[{
        "name":"symbol",
        "type":"str",
        "required":true,
        "description":"symbol='603777'; 股票代码可以在 ak.stock_zh_a_spot_em() 中获取"
    },
    {
        "name":"period",
        "type":"str",
        "required":true,
        "description":"period='daily'; choice of {'daily', 'weekly', 'monthly'}"
    },
    {
        "name":"start_date",
        "type":"str",
        "required":true,
        "description":"start_date='20210301'; 开始查询的日期"
    },
    {
        "name":"end_date",
        "type":"str",
        "required":true,
        "description":"end_date='20210616'; 结束查询的日期"
    },
    {
        "name":"adjust",
        "type":"str",
        "required":false,
        "description":"默认返回不复权的数据; qfq: 返回前复权后的数据; hfq: 返回后复权后的数据"
    },
    {
        "name":"timeout",
        "type":"float",
        "required":false,
        "description":"timeout=None; 默认不设置超时参数"
    }
    ],
    "output":[{
        "name":"日期",
        "type":"object",
        "description":"交易日",
        "predicate_pushdown" : true
    },
    {
        "name":"股票代码",
        "type":"object",
        "description":"不带市场标识的股票代码"
    },
    {
        "name":"开盘",
        "type":"float64",
        "description":"开盘价"
    },
    {
        "name":"开盘",
        "type":"float64",
        "description":"开盘价"
    },{
        "name":"收盘",
        "type":"float64",
        "description":"收盘价"
    },{
        "name":"最高",
        "type":"float64",
        "description":"最高价"
    },{
        "name":"最低",
        "type":"float64",
        "description":"最低价"
    },{
        "name":"成交量",
        "type":"float64",
        "description":"单位：手"
    },{
        "name":"成交额",
        "type":"float64",
        "description":"单位：元"
    },{
        "name":"振幅",
        "type":"float64",
        "description":"单位：%"
    },{
        "name":"涨跌幅",
        "type":"float64",
        "description":"单位：%"
    },{
        "name":"涨跌额",
        "type":"float64",
        "description":"单位：元"
    },{
        "name":"换手率",
        "type":"float64",
        "description":"单位：%"
    }
    ]
}